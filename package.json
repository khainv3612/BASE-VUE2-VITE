  "lint-staged": {
    "*.{js,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{js,vue}": [
      "prettier src  --write",
      "eslint --ext \"**/*.{js,vue}\" --ignore-path .gitignore --fix",
      "eslint --cache"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
